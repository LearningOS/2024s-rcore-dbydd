{
	"name": "RCore Tutorial Dev Container",
	"build": {
		"dockerfile": "Dockerfile",
		"args": {
			"VARIANT": "latest"
		}
	},
	"overrideCommand": false,
	"remoteUser": "root",
	"containerEnv": {
		"SHELL": "/usr/bin/fish"
	},
	"settings": {
		"terminal.integrated.defaultProfile.linux": "/usr/bin/fish"
	},
	"extensions": [
		"rust-lang.rust"
	],
	"remoteUser": "root"
}